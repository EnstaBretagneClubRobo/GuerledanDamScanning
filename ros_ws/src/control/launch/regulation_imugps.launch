<!-- Mission de suivi de waypoints -->
<!-- <rosparam file="$(find sensors)/config/imu_boat.yaml" command="load"/> -->
<launch>
	<arg name="pololu" default="0"/>
    <rosparam command="load" file="$(find control)/mission_configs/barr2.yaml" />

    <node pkg="processing" type="err_dist_with_gps.py" name="err_dist_with_gps" output="screen">
    </node>

    <node pkg="processing" type="err_alpha_with_imu.py" name="err_alpha_with_imu" output="screen">
    </node>

    <node pkg="control" type="regulation_from_err_alpha_dist.py" name="regulation" output="screen">
    </node>

    <node pkg="actuators" type="differential_real.py" name="cmd_to_diff" output="screen">
    </node>

    <node pkg="actuators" type="motor_controller.py" name="motor_controller">
    	<param name="sPort" type="string" value="/dev/pololu"/>
    	<param name="port" type="string" value="$(arg pololu)"/>
    </node>

</launch>
